<?xml version="1.0" encoding="UTF-8"?>
<Window
        caption="0,0,0,28" use_system_caption="false" snap_layout_menu="true" sys_menu="true" sys_menu_rect="0,0,36,36"
        shadow_type="default" shadow_attached="true" shadow_snap="true" layered_window="true"
        alpha="255" icon="public/caption/logo.ico"
		
		xmlns:xs="http://www.w3.org/2001/XMLSchema"
		xmlns="http://tempuri.org/NIMDUISchema.xsd"
		>
	<VBox bkcolor="panelBkColor" visible="true" height="auto" width="auto">
		<!-- 标题栏区域 -->
		<HBox name="window_caption_bar" width="stretch" height="28">
			<Label name="text_Title" text_align="vcenter"  margin="8,0,8,0" valign="center" font="system_14"/>
			<Control />

			<Button class="btn_wnd_close_11" height="stretch" width="40" name="btn_Close" margin="0,0,0,2"/>
		</HBox>
		<Control height="1" bkcolor="itemSeparatorColor"/>
		<!-- 工作区域，除了标题栏外的内容都放在这个大的Box区域 -->
		<VBox height="auto" width="auto" min_width="280">
			<Control height="30"/>
			<Label name="text_Content" halign="center" margin="24,0,24,0" text_align="center" font="system_16"/>
			<Control height="30"/>
			<!--按钮区-->
			<HBox margin="8,8,8,8"  halign="center" height="auto">
				<Control height="30"/>
				<Button class="ctb_btn_dlg_normal" name="btn_Cancel" text_id="STRING_CANCEL" visible="false"/>
				<Button class="ctb_btn_dlg_highlight" name="btn_OK" text_id="STRING_OK" visible="false"/>
			</HBox>
		</VBox>



	</VBox>
</Window>